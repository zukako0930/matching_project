<p id="notice"><%= notice %></p>

<h1>Meet Requests</h1>

<table>
  <thead>
    <tr>
      <th>Request user</th>
      <th>Target user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meet_requests.each do |meet_request| %>
      <tr>
        <td><%= meet_request.meet_request_user_id %></td>
        <td><%= meet_request.meet_target_user_id %></td>
        <td><%= link_to 'Show', meet_request %></td>
        <td><%= link_to 'Edit', edit_meet_request_path(meet_request) %></td>
        <td><%= link_to 'Destroy', meet_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meet Request', new_meet_request_path %>
